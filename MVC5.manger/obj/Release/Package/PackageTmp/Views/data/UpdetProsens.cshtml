
@{
    ViewBag.Title = "UpdetProsens";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>个人信息</title>
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="format-detection" content="telephone=no">
    <link href="~/022/common/layui/css/layui.css" rel="stylesheet" />
    <link rel="stylesheet" type="~/022/text/css" href="common/bootstrap/css/bootstrap.css" media="all">
    <link rel="stylesheet" type="~/022/text/css" href="common/global.css" media="all">
    <link rel="stylesheet" type="~/022/text/css" href="css/personal.css" media="all">
    <style type="text/css">
        * {
            margin: 0 auto;
            padding: 0;
            border: 0;
        }

        .box {
            width: 40%;
            margin-top: 20px;
            margin: 0 auto;
        }

        #titel {
            font-size: 1.5em;
            width: 50%;
            text-align: center;
            margin-top: 30px;
        }

        #textss {
            margin-top: 40px;
        }
         input {
            width: 300px;
        }

        .layui1 {
            display: block;
            width: 300px;
            margin-left: 100px;
        }

        .layui-btn1 {
            display: inline-block;
            height: 38px;
            line-height: 38px;
            padding: 0 18px;
            background-color: #009688;
            color: #fff;
            white-space: nowrap;
            text-align: center;
            border: none;
            border-radius: 2px;
            cursor: pointer;
            opacity: .9;
        }

        .remove {
            height: 38px;
            line-height: 38px;
            width: 80px;
            border: 1px solid #C9C9C9;
            background-color: #fff;
            color: #555;
            margin-left: 100px;
        }
    </style>
</head>
<body>
    <div class="box">
        <section class="layui-larry-box">
            <div class="larry-personal" id="prosen">
                <header class="larry-personal-tit" id="titel">
                    <h2 style="margin-left:45%">修改个人信息</h2>
                </header>
                <div class="larry-personal-body clearfix" id="textss">
                    <form class="layui-form col-lg-5" id="updetPerson">
                        <div class="layui-form-item">
                            <label class="layui-form-label">用户Id</label>
                            <div class="layui-input-block">
                                <input type="text" name="userID" autocomplete="" class="layui-input layui-disabled" id="userID" readonly="">
                            </div>
                        </div>
                        <div class="layui-form-item">
                            <label class="layui-form-label">用户名</label>
                            <div class="layui-input-block">
                                <input type="text" name="userLog" autocomplete="" class="layui-input layui-disabled" id="userLog" readonly="">
                            </div>
                        </div>
                        <div class="layui-form-item">
                            <label class="layui-form-label">姓名</label>
                            <div class="layui-input-block">
                                <input type="text" name="userName" id="userName" autocomplete="" class="layui-input layui-disabled" readonly="">
                            </div>
                        </div>
                        <div class="layui-form-item">
                            <label class="layui-form-label">性别</label>
                            <div class="layui-input-block">
                                <input type="text" name="userSex" id="userSex" autocomplete="off" class="layui-input layui-disabled" readonly="">
                            </div>
                        </div>
                        <div class="layui-form-item">
                            <label class="layui-form-label">手机号码</label>
                            <div class="layui-input-block">
                                <input type="text" name="userPhone" id="userPhone" autocomplete="off" class="layui-input">
                            </div>
                        </div>
                        <div class="layui-form-item">
                            <label class="layui-form-label">用户类型</label>
                            <div class="layui-input-block">

                                <input type="text" name="userType" id="userType" autocomplete="" class="layui-input">

                            </div>
                        </div>
                        <div class="layui-form-item">
                            <label class="layui-form-label">用户地址</label>
                            <div class="layui-input-block">
                                <input type="text" name="userAderrs" id="userAderrs" autocomplete="off" class="layui-input ">
                            </div>
                        </div>

                    </form>
                </div>

            </div>
        </section>
        <div class="layui1">
            <button class="layui-btn1" type="button" onclick="btn_ok()">立即提交</button>

            <button type="reset" class="remove" onclick="btn_cancel()">取消</button>
        </div>
    </div>
        <script src="~/Scripts/jquery-3.3.1.min.js"></script>
        <script src="~/022/common/layui/layui.js"></script>
        <script src="~/Scripts/data/updetPPorsen.js"></script>
</body>
</html>

